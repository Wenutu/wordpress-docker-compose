[mysqld]
# Disable Query Cache (Deprecated in newer MariaDB versions, keeping 0 to ensure disabled)
query_cache_type = 0
query_cache_size = 0

# InnoDB Buffer Pool Size
# Set to 512M for better performance, adjust based on available RAM
innodb_buffer_pool_size = 512M

# InnoDB Log File Size
innodb_log_file_size = 128M

# Max Connections
max_connections = 500

# Connection Timeouts (in seconds)
wait_timeout = 600
interactive_timeout = 600
